{
  "name": "frontend-greeting-next",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "next dev",
    "build": "next build",
    "start": "next start",
    "lint": "eslint . --ext .js,.jsx,.ts,.tsx",
    "format": "prettier --write './src/**/*.{js,jsx,ts,tsx}'",
    "deploy:firebase:init": "firebase login && firebase use --add",
    "deploy:firebase": "firebase deploy --only hosting",
    "deploy:walrus:testnet": "pnpm build && pnpx walrus-sites-deploy -n testnet",
    "deploy:walrus:mainnet": "pnpm build && pnpx walrus-sites-deploy -n mainnet",
    "deploy:arweave": "ARWEAVE_DEPLOYMENT=true pnpm run build && pnpx arkb deploy ./dist -w ./arweave-keyfile.json --auto-confirm"
  },
  "dependencies": {
    "@mysten/dapp-kit": "^0.19.11",
    "@mysten/sui": "^1.45.2",
    "@mysten/wallet-standard": "^0.19.9",
    "@radix-ui/react-select": "^2.2.6",
    "@radix-ui/react-toggle": "^1.1.10",
    "@radix-ui/themes": "^3.2.1",
    "@suiware/kit": "^0.11.1",
    "@tanstack/react-query": "^5.90.12",
    "clsx": "^2.1.1",
    "lucide-react": "^0.561.0",
    "next": "16.0.10",
    "next-themes": "^0.4.6",
    "react": "^19.2.3",
    "react-dom": "^19.2.3",
    "react-hot-toast": "^2.6.0",
    "react-use": "^17.6.0"
  },
  "devDependencies": {
    "@eslint/eslintrc": "^3.3.3",
    "@eslint/js": "^9.39.1",
    "@types/node": "^25.0.1",
    "@types/react": "^19.2.7",
