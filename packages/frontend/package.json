{
  "name": "frontend",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "dev": "vite --open",
    "build": "tsc && vite build",
    "lint": "ESLINT_USE_FLAT_CONFIG=false eslint . --ext ts,tsx --report-unused-disable-directives --max-warnings 0",
    "preview": "vite preview",
    "format": "prettier --write './index.html' './src/**/*.{js,jsx,ts,tsx}'",
    "deploy:firebase:init": "firebase login && firebase use --add",
    "deploy:firebase": "firebase deploy --only hosting",
    "deploy:walrus:testnet": "pnpm build && pnpx walrus-sites-deploy -n testnet",
    "deploy:walrus:mainnet": "pnpm build && pnpx walrus-sites-deploy -n mainnet",
    "deploy:arweave": "ARWEAVE_DEPLOYMENT=true pnpm run build && pnpx arkb deploy ./dist -w ./arweave-keyfile.json --auto-confirm"
  },
  "dependencies": {
    "@mysten/dapp-kit": "^0.19.11",
    "@mysten/sui": "^1.45.2",
    "@mysten/wallet-standard": "^0.19.9",
    "@radix-ui/react-select": "^2.2.6",
    "@radix-ui/react-toggle": "^1.1.10",
    "@radix-ui/themes": "^3.2.1",
    "@suiware/kit": "^0.11.1",
    "@tanstack/react-query": "^5.90.12",
    "clsx": "^2.1.1",
    "lucide-react": "^0.555.0",
    "react": "^19.2.3",
    "react-dom": "^19.2.3",
    "react-hot-toast": "^2.6.0",
    "react-use": "^17.6.0"
  },
  "devDependencies": {
    "@types/node": "^24.10.3",
    "@types/react": "^19.2.7",
    "@types/react-dom": "^19.2.3",
    "@typescript-eslint/eslint-plugin": "^8.49.0",
    "@typescript-eslint/parser": "^8.49.0",
    "@vitejs/plugin-react-swc": "^4.2.2",
    "autoprefixer": "^10.4.22",
    "eslint": "^9.39.1",
    "eslint-config-prettier": "^10.1.8",
    "eslint-plugin-react-hooks": "^7.0.1",
    "eslint-plugin-react-refresh": "^0.4.24",
    "postcss": "^8.5.6",
    "prettier": "^3.7.4",
    "prettier-plugin-tailwindcss": "^0.7.2",
    "tailwindcss": "^3.4.19",
    "typescript": "^5.9.3",
    "vite": "^7.2.7",
    "vite-tsconfig-paths": "^5.1.4"
  }
}
