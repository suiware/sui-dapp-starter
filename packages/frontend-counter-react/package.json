{
  "name": "frontend-counter-react",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "dev": "vite --open",
    "build": "tsc && vite build",
    "lint": "ESLINT_USE_FLAT_CONFIG=false eslint . --ext ts,tsx --report-unused-disable-directives --max-warnings 0",
    "preview": "vite preview",
    "format": "prettier --write './index.html' './src/**/*.{js,jsx,ts,tsx}'",
    "deploy:firebase:init": "firebase login && firebase use --add",
    "deploy:firebase": "firebase deploy --only hosting",
    "deploy:walrus:testnet": "pnpm build && pnpx walrus-sites-deploy -n testnet",
    "deploy:walrus:mainnet": "pnpm build && pnpx walrus-sites-deploy -n mainnet",
    "deploy:arweave": "ARWEAVE_DEPLOYMENT=true pnpm run build && pnpx arkb deploy ./dist -w ./arweave-keyfile.json --auto-confirm"
  },
  "dependencies": {
    "@mysten/dapp-kit": "^0.14.53",
    "@mysten/sui": "^1.27.1",
    "@mysten/wallet-standard": "^0.13.29",
    "@radix-ui/react-select": "^2.1.7",
    "@radix-ui/react-toggle": "^1.1.3",
    "@radix-ui/themes": "^3.2.1",
    "@suiware/kit": "^0.8.1",
    "@tanstack/react-query": "^5.74.4",
    "clsx": "^2.1.1",
    "lucide-react": "^0.473.0",
    "react": "^19.1.0",
    "react-dom": "^19.1.0",
    "react-hot-toast": "^2.5.2",
    "react-router": "^7.5.0",
    "react-use": "^17.6.0"
  },
  "devDependencies": {
    "@types/node": "^22.14.1",
    "@types/react": "^19.1.2",
    "@types/react-dom": "^19.1.2",
    "@typescript-eslint/eslint-plugin": "^8.30.1",
    "@typescript-eslint/parser": "^8.30.1",
    "@vitejs/plugin-react-swc": "^3.9.0",
    "autoprefixer": "^10.4.21",
    "eslint": "^9.24.0",
    "eslint-config-prettier": "^10.1.2",
    "eslint-plugin-react-hooks": "^5.2.0",
    "eslint-plugin-react-refresh": "^0.4.19",
    "postcss": "^8.5.3",
    "prettier": "^3.5.3",
    "prettier-plugin-tailwindcss": "^0.6.11",
    "tailwindcss": "^3.4.17",
    "typescript": "^5.8.3",
    "vite": "^6.3.1",
    "vite-tsconfig-paths": "^5.1.4"
  }
}
